{% extends "layout.njk" %}

{% block head %}
    <title>{{about.name}}</title>
{% endblock %}

{% block content %}

<div id="wrapper">
    <img src="{{about.avatar_url}}" alt="Imagem de {{about.name}}" >
    <h1>{{about.name}}</h1>   
    <p>{{about.role}}</p>
</div>

<div id="lists">
    <ul>
        {% for list in about.lists %}
            <li>{{ list.name }}</li>
        {% endfor %}
    </ul>
</div>

<div id="links-footer" class="links">
    {% for link in about.links %}
        <a href="{{ link.url }}" target="_blank">{{ link.name }}</a>
    {% endfor %}
</div>

{% endblock%}